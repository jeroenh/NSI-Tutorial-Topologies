@prefix dominica: <urn:ogf:network:ets.dominica.ex:2013:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:ets.bonaire.ex:2013> a nml:Topology;
    nsi:isReference "https://raw.github.com/jeroenh/NSI-Tutorial-Topologies/master/bonaire.n3" .

<urn:ogf:network:ets.dominica.ex:2013> a nml:Topology;
    nml:hasBidirectionalPort dominica:port10,
        dominica:port7;
    nml:hasInboundPort dominica:port10-in,
        dominica:port7-in;
    nml:hasOutboundPort dominica:port10-out,
        dominica:port7-out;
    nml:locatedAt dominica:location;
    nsi:managedBy dominica:nsa .

dominica:location a nml:Location .

dominica:nsa a nsi:NSA;
    nsi:csProviderEndpoint "http://192.168.1.11:28380/nsi2_gl_proxy/services/ConnectionProvider";
    nsi:managing <urn:ogf:network:ets.dominica.ex:2013> .

dominica:port10 a nml:BidirectionalPort;
    nml:hasPort dominica:port10-in,
        dominica:port10-out .

dominica:port7 a nml:BidirectionalPort;
    nml:hasPort dominica:port7-in,
        dominica:port7-out .

dominica:port10-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:ets.bonaire.ex:2013:port10-out>;
    nmleth:vlans "1780-1799" .

dominica:port10-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:ets.bonaire.ex:2013:port10-in>;
    nmleth:vlans "1780-1799" .

dominica:port7-in a nml:PortGroup;
    nmleth:vlans "1780-1799" .

dominica:port7-out a nml:PortGroup;
    nmleth:vlans "1780-1799" .

