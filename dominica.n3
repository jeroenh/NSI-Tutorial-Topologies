@prefix dominica: <urn:ogf:network:dominica:2013:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:bonaire:2013:topology> a nml:Topology .

<urn:ogf:network:curacao:2013:topology> a nml:Topology .

dominica:bi-dominica a nml:BidirectionalPort;
    nml:hasPort dominica:port15-in,
        dominica:port15-out,
        dominica:port25-in,
        dominica:port25-out .

dominica:location a nml:Location .

dominica:nsa a nsi:NSA;
    nsi:managing dominica:topology .

dominica:topology a nml:Topology;
    nml:hasBidirectionalPort dominica:bi-dominica;
    nml:hasInboundPort dominica:port15-in,
        dominica:port25-in;
    nml:hasOutboundPort dominica:port15-out,
        dominica:port25-out;
    nml:locatedAt dominica:location;
    nsi:managedBy dominica:nsa .

dominica:port15-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:curacao:2013:port25-out>;
    nmleth:vlans "1780-1799" .

dominica:port15-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:curacao:2013:port25-in>;
    nmleth:vlans "1780-1799" .

dominica:port25-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:bonaire:2013:port25-out>;
    nmleth:vlans "1780-1799" .

dominica:port25-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:bonaire:2013:port25-in>;
    nmleth:vlans "1780-1799" .

