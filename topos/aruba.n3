@prefix aruba: <urn:ogf:network:ets.aruba.ex:2013:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:ets.bonaire.ex:2013> a nml:Topology;
    nsi:isReference "https://raw.github.com/jeroenh/NSI-Tutorial-Topologies/master/topos/bonaire.n3" .

<urn:ogf:network:ets.curacao.ex:2013> a nml:Topology;
    nsi:isReference "https://raw.github.com/jeroenh/NSI-Tutorial-Topologies/master/topos/curacao.n3" .

<urn:ogf:network:ets.aruba.ex:2013> a nml:Topology;
    nml:hasBidirectionalPort aruba:port10,
        aruba:port15,
        aruba:port20;
    nml:hasInboundPort aruba:port10-in,
        aruba:port15-in,
        aruba:port20-in;
    nml:hasOutboundPort aruba:port10-out,
        aruba:port15-out,
        aruba:port20-out;
    nml:locatedAt aruba:location;
    nsi:managedBy aruba:nsa .

aruba:location a nml:Location .

aruba:nsa a nsi:NSA;
    nsi:csProviderEndpoint "http://192.168.1.11:28380/nsi2_gl_proxy/services/ConnectionProvider";
    nsi:managing <urn:ogf:network:ets.aruba.ex:2013> .

aruba:port10 a nml:BidirectionalPort;
    nml:hasPort aruba:port10-in,
        aruba:port10-out .

aruba:port15 a nml:BidirectionalPort;
    nml:hasPort aruba:port15-in,
        aruba:port15-out .

aruba:port20 a nml:BidirectionalPort;
    nml:hasPort aruba:port20-in,
        aruba:port20-out .

aruba:port10-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:ets.curacao.ex:2013:port10-out>;
    nmleth:vlans "1780-1799" .

aruba:port10-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:ets.curacao.ex:2013:port10-in>;
    nmleth:vlans "1780-1799" .

aruba:port15-in a nml:PortGroup;
    nmleth:vlans "1780-1799" .

aruba:port15-out a nml:PortGroup;
    nmleth:vlans "1780-1799" .

aruba:port20-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:ets.bonaire.ex:2013:port20-out>;
    nmleth:vlans "1780-1799" .

aruba:port20-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:ets.bonaire.ex:2013:port20-in>;
    nmleth:vlans "1780-1799" .

