@prefix curacao: <urn:ogf:network:ets.curacao.ex:2013:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:ets.aruba.ex:2013> a nml:Topology;
    nsi:isReference "https://raw.github.com/jeroenh/NSI-Tutorial-Topologies/master/aruba.n3" .

<urn:ogf:network:ets.curacao.ex:2013> a nml:Topology;
    nml:hasBidirectionalPort curacao:port10,
        curacao:port20,
        curacao:port4;
    nml:hasInboundPort curacao:port10-in,
        curacao:port20-in,
        curacao:port4-in;
    nml:hasOutboundPort curacao:port10-out,
        curacao:port20-out,
        curacao:port4-out;
    nml:locatedAt curacao:location;
    nsi:managedBy curacao:nsa .

curacao:location a nml:Location .

curacao:nsa a nsi:NSA;
    nsi:csProviderEndpoint "http://192.168.1.14:8288/ConnectionService";
    nsi:managing <urn:ogf:network:ets.curacao.ex:2013> .

curacao:port10 a nml:BidirectionalPort;
    nml:hasPort curacao:port10-in,
        curacao:port10-out .

curacao:port20 a nml:BidirectionalPort;
    nml:hasPort curacao:port20-in,
        curacao:port20-out .

curacao:port4 a nml:BidirectionalPort;
    nml:hasPort curacao:port4-in,
        curacao:port4-out .

curacao:port10-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:ets.aruba.ex:2013:port10-out>;
    nmleth:vlans "1780-1799" .

curacao:port10-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:ets.aruba.ex:2013:port10-in>;
    nmleth:vlans "1780-1799" .

curacao:port20-in a nml:PortGroup;
    nmleth:vlans "1780-1799" .

curacao:port20-out a nml:PortGroup;
    nmleth:vlans "1780-1799" .

curacao:port4-in a nml:PortGroup;
    nmleth:vlans "1780-1799" .

curacao:port4-out a nml:PortGroup;
    nmleth:vlans "1780-1799" .

