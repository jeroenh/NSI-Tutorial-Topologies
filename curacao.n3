@prefix curacao: <urn:ogf:network:curacao:2013:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:aruba:2013:topology> a nml:Topology .

curacao:bi-curacao a nml:BidirectionalPort;
    nml:hasPort curacao:port10-in,
        curacao:port10-out,
        curacao:port20-in,
        curacao:port20-out,
        curacao:port4-in,
        curacao:port4-out .

curacao:location a nml:Location .

curacao:nsa a nsi:NSA;
    nsi:csProviderEndpoint "http://192.168.1.153:8288/ConnectionService";
    nsi:managing curacao:topology .

curacao:topology a nml:Topology;
    nml:hasBidirectionalPort curacao:bi-curacao;
    nml:hasInboundPort curacao:port10-in,
        curacao:port20-in,
        curacao:port4-in;
    nml:hasOutboundPort curacao:port10-out,
        curacao:port20-out,
        curacao:port4-out;
    nml:locatedAt curacao:location;
    nsi:managedBy curacao:nsa .

curacao:port10-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:aruba:2013:port10-out>;
    nmleth:vlans "1780-1799" .

curacao:port10-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:aruba:2013:port10-in>;
    nmleth:vlans "1780-1799" .

curacao:port20-in a nml:PortGroup;
    nmleth:vlans "1780-1799" .

curacao:port20-out a nml:PortGroup;
    nmleth:vlans "1780-1799" .

curacao:port4-in a nml:PortGroup;
    nmleth:vlans "1780-1799" .

curacao:port4-out a nml:PortGroup;
    nmleth:vlans "1780-1799" .

