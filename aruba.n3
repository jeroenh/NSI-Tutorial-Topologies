@prefix aruba: <urn:ogf:network:aruba:2013:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:bonaire:2013:topology> a nml:Topology .

<urn:ogf:network:curacao:2013:topology> a nml:Topology .

aruba:bi-aruba a nml:BidirectionalPort;
    nml:hasPort aruba:port10-in,
        aruba:port10-out,
        aruba:port20-in,
        aruba:port20-out .

aruba:location a nml:Location .

aruba:nsa a nsi:NSA;
    nsi:managing aruba:topology .

aruba:topology a nml:Topology;
    nml:hasBidirectionalPort aruba:bi-aruba;
    nml:hasInboundPort aruba:port10-in,
        aruba:port20-in;
    nml:hasOutboundPort aruba:port10-out,
        aruba:port20-out;
    nml:locatedAt aruba:location;
    nsi:managedBy aruba:nsa .

aruba:port10-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:bonaire:2013:port10-out>;
    nmleth:vlans "1780-1799" .

aruba:port10-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:bonaire:2013:port10-in>;
    nmleth:vlans "1780-1799" .

aruba:port20-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:curacao:2013:port10-out>;
    nmleth:vlans "1780-1799" .

aruba:port20-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:curacao:2013:port10-in>;
    nmleth:vlans "1780-1799" .

