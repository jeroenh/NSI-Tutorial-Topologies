@prefix bonaire: <urn:ogf:network:bonaire:2013:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:dominica:2013:topology> a nml:Topology;
    nsi:isReference "https://raw.github.com/jeroenh/NSI-Tutorial-Topologies/master/dominica.n3" .

bonaire:bi-bonaire a nml:BidirectionalPort;
    nml:hasPort bonaire:port10-in,
        bonaire:port10-out,
        bonaire:port22-in,
        bonaire:port22-out,
        bonaire:port7-in,
        bonaire:port7-out .

bonaire:location a nml:Location .

bonaire:nsa a nsi:NSA;
    nsi:csProviderEndpoint "http://192.168.1.152:9080/NSI/services/CS2";
    nsi:managing bonaire:topology .

bonaire:topology a nml:Topology;
    nml:hasBidirectionalPort bonaire:bi-bonaire;
    nml:hasInboundPort bonaire:port10-in,
        bonaire:port22-in,
        bonaire:port7-in;
    nml:hasOutboundPort bonaire:port10-out,
        bonaire:port22-out,
        bonaire:port7-out;
    nml:locatedAt bonaire:location;
    nsi:managedBy bonaire:nsa .

bonaire:port10-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:dominica:2013:port10-out>;
    nmleth:vlans "1780-1799" .

bonaire:port10-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:dominica:2013:port10-in>;
    nmleth:vlans "1780-1799" .

bonaire:port22-in a nml:PortGroup;
    nmleth:vlans "1780-1799" .

bonaire:port22-out a nml:PortGroup;
    nmleth:vlans "1780-1799" .

bonaire:port7-in a nml:PortGroup;
    nmleth:vlans "1780-1799" .

bonaire:port7-out a nml:PortGroup;
    nmleth:vlans "1780-1799" .

