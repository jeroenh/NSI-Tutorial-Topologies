@prefix bonaire: <urn:ogf:network:bonaire:2013:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:aruba:2013:topology> a nml:Topology .

<urn:ogf:network:dominica:2013:topology> a nml:Topology .

bonaire:bi-bonaire-aruba a nml:BidirectionalPort;
    nml:hasPort bonaire:aruba-bonaire,
        bonaire:bonaire-aruba .

bonaire:bi-bonaire-dominica a nml:BidirectionalPort;
    nml:hasPort bonaire:bonaire-dominica,
        bonaire:dominica-bonaire .

bonaire:location a nml:Location .

bonaire:nsa a nsi:NSA;
    nsi:managing bonaire:topology .

bonaire:topology a nml:Topology;
    nml:hasBidirectionalPort bonaire:bi-bonaire-aruba,
        bonaire:bi-bonaire-dominica;
    nml:hasInboundPort bonaire:aruba-bonaire,
        bonaire:dominica-bonaire;
    nml:hasOutboundPort bonaire:bonaire-aruba,
        bonaire:bonaire-dominica;
    nml:locatedAt bonaire:location;
    nsi:managedBy bonaire:nsa .

bonaire:aruba-bonaire a nml:PortGroup;
    nml:isAlias <urn:ogf:network:aruba:2013:aruba-bonaire>;
    nmleth:vlans "1780-1799" .

bonaire:bonaire-aruba a nml:PortGroup;
    nml:isAlias <urn:ogf:network:aruba:2013:bonaire-aruba>;
    nmleth:vlans "1780-1799" .

bonaire:bonaire-dominica a nml:PortGroup;
    nml:isAlias <urn:ogf:network:dominica:2013:bonaire-dominica>;
    nmleth:vlans "1780-1799" .

bonaire:dominica-bonaire a nml:PortGroup;
    nml:isAlias <urn:ogf:network:dominica:2013:dominica-bonaire>;
    nmleth:vlans "1780-1799" .

