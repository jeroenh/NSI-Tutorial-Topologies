@prefix bonaire: <urn:ogf:network:bonaire:2013:> .
@prefix nml: <http://schemas.ogf.org/nml/2012/10/base#> .
@prefix nmleth: <http://schemas.ogf.org/nml/2012/10/ethernet#> .
@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

<urn:ogf:network:aruba:2013:topology> a nml:Topology .

<urn:ogf:network:dominica:2013:topology> a nml:Topology .

bonaire:bi-bonaire a nml:BidirectionalPort;
    nml:hasPort bonaire:port15-in,
        bonaire:port15-out,
        bonaire:port20-in,
        bonaire:port20-out .

bonaire:location a nml:Location .

bonaire:nsa a nsi:NSA;
    nsi:managing bonaire:topology .

bonaire:topology a nml:Topology;
    nml:hasBidirectionalPort bonaire:bi-bonaire;
    nml:hasInboundPort bonaire:port15-in,
        bonaire:port20-in;
    nml:hasOutboundPort bonaire:port15-out,
        bonaire:port20-out;
    nml:locatedAt bonaire:location;
    nsi:managedBy bonaire:nsa .

bonaire:port15-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:aruba:2013:port15-out>;
    nmleth:vlans "1780-1799" .

bonaire:port15-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:aruba:2013:port15-in>;
    nmleth:vlans "1780-1799" .

bonaire:port20-in a nml:PortGroup;
    nml:isAlias <urn:ogf:network:dominica:2013:port20-out>;
    nmleth:vlans "1780-1799" .

bonaire:port20-out a nml:PortGroup;
    nml:isAlias <urn:ogf:network:dominica:2013:port20-in>;
    nmleth:vlans "1780-1799" .

