@prefix nsi: <http://schemas.ogf.org/nsi/2012/10/topology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

nmleth:vlans owl:subPropertyOf nml:hasLabelGroup .

aruba:bi-aruba a nml:BidirectionalPort;
    nml:hasPort aruba:port10-in,
        aruba:port10-out,
        aruba:port15-in,
        aruba:port15-out .

aruba:location a nml:Location .

aruba:nsa a nsi:NSA;
    nsi:csProviderEndpoint "http://192.168.1.11:28380/nsi2_gl_proxy/services/ConnectionProvider";
    nsi:managing aruba:topology .

aruba:topology a nml:Topology;
    nml:hasBidirectionalPort aruba:bi-aruba;
    nml:hasInboundPort aruba:port10-in,
        aruba:port15-in;
    nml:hasOutboundPort aruba:port10-out,
        aruba:port15-out;
    nml:locatedAt aruba:location;
    nsi:managedBy aruba:nsa .

